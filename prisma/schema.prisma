generator client {
  provider = "prisma-client-js"
  output   = "./client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id      Int     @id @default(autoincrement())
  title   String
  content String?
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String?
  password String  @db.Char(255)
}
